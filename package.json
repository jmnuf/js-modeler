    "@typescript-eslint/parser": "^4.33.0",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.4.1",
    "jest": "^26.6.3",
    "prettier": "^2.7.1",
    "ts-jest": "^26.5.6",
    "ts-loader": "^8.4.0",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-node-externals": "^2.5.2"
